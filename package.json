{
  "name": "@mtti/mhp-example",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run watch\" \"mhp serve --verbose\"",
    "watch": "chokidar --initial -c \"npm run build:local\" \"mhprc.js\" \"posts/**/*.md\" \"templates/**/*.html\" \"assets/**/*\" \"pages\"",
    "build": "npm run build:assets && npm run build:content",
    "build:local": "npm run build:assets && npm run build:content:local",
    "build:assets": "webpack",
    "build:assets:development": "webpack --mode=development",
    "build:content": "mhp",
    "build:content:local": "mhp build --var \"baseUrl=http://localhost:8080\"",
    "test:content": "npm run build:content && node test/scripts.js compare",
    "test:content:snapshot": "node test/scripts.js save",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@mtti/mhp": "0.2.3",
    "@types/node": "12.12.21",
    "axios": "0.19.0",
    "bootstrap": "4.4.1",
    "chokidar-cli": "2.1.0",
    "concurrently": "5.0.2",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.0",
    "file-loader": "5.0.2",
    "html-loader": "0.5.5",
    "jquery": "3.5.0",
    "mini-css-extract-plugin": "0.8.2",
    "node-sass": "4.13.0",
    "popper.js": "1.16.0",
    "prop-types": "15.7.2",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.2",
    "ts-loader": "6.2.1",
    "typescript": "3.7.3",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.0",
    "webpack-manifest-plugin": "2.2.0"
  }
}
