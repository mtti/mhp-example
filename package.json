{
  "name": "@mtti/mhp-example",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run watch\" \"mhp serve --verbose\"",
    "watch": "watch \"npm run build:local\" posts templates assets pages",
    "build": "npm run build:frontend && npm run build:backend",
    "build:local": "npm run build:frontend && npm run build:backend:local",
    "build:frontend": "webpack",
    "build:frontend:development": "webpack --mode=development",
    "build:backend": "mhp --verbose --keep=assets",
    "build:backend:local": "mhp --verbose --keep=assets --baseUrl=\"http://localhost:8080\"",
    "test:backend": "mhp --keep=assets && node test/scripts.js compare",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "axios": "0.18.0",
    "@babel/core": "7.4.4",
    "babel-loader": "8.0.6",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "bootstrap": "4.3.1",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jquery": "3.4.1",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.12.0",
    "popper.js": "1.15.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "watch": "1.0.2",
    "webpack": "4.32.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-manifest-plugin": "2.0.4"
  }
}
